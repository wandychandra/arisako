{
  "name": "arisako",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "node": "hardhat node",
    "deploy:local": "hardhat run scripts/deploy/deploy.js --network localhost",
    "deploy:sepolia": "hardhat run scripts/deploy/deploy.js --network baseSepolia",
    "deploy:mainnet": "hardhat run scripts/deploy/deploy.js --network baseMainnet",
    "verify:sepolia": "hardhat verify --network baseSepolia",
    "clean": "hardhat clean && rm -rf .next out",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@chainlink/contracts": "^1.5.0",
    "@coinbase/onchainkit": "1.1.2",
    "@openzeppelin/contracts": "^5.4.0",
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.81.5",
    "@wagmi/connectors": "^5.0.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "next": "16.1.1",
    "openai": "^6.15.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "viem": "^2.43.3",
    "wagmi": "^2.16.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.4",
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "hardhat": "^2.28.1",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "supabase": "^2.70.5",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}